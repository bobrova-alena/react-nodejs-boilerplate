{
  "extends": "../../package.json",
  "main": "server.js",
  "scripts": {
    "watch": "nodemon ./dist/serverApp.js localhost 3080",
    "----------": "",
    "start": "node ./dist/serverApp.js",
    "build:dev": "cross-env MODE_ENV=development webpack --progress --color --mode development --config ./webpack/webpack.js",
    "build:prod": "cross-env webpack --color --mode production --config ./webpack/webpack.js",
    "debug:build:dev": "cross-env MODE_ENV=development DEBUG_ENV=debug webpack --progress --color --mode development --config ./webpack/webpack.js",
    "eslint": "eslint --fix"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.4",
    "ts-loader": "^9.2.3",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.7.2",
    "webpack-stream": "^6.1.2"
  }
}
